    {
      "parameters": {
        "jsCode": "// Código para separar as objeções do Curso Profissão Sorveteiro no n8n\nconst dados = $input.first().json;\n\n// Se for um array, pega o primeiro item\nconst item = Array.isArray(dados) ? dados[0] : dados;\n\n// Extrai o conteúdo\nconst textoCompleto = item.content || item.texto || item.data;\n\nif (!textoCompleto || typeof textoCompleto !== 'string') {\n  throw new Error(`Texto não encontrado. Estrutura: ${JSON.stringify(Object.keys(item))}`);\n}\n\n// Remove o título principal e normaliza quebras de linha\nconst textoLimpo = textoCompleto\n  .replace(/^OBJEÇÕES COMUNS E RESPOSTAS PARA O CURSO PROFISSÃO SORVETEIRO\\s*\\n+/, '')\n  .replace(/\\u000b/g, '\\n') // Remove caracteres especiais de quebra\n  .replace(/\\n{3,}/g, '\\n\\n'); // Normaliza múltiplas quebras\n\n// Separa usando regex mais específica para capturar números no início da linha\nconst matches = textoLimpo.match(/\\d+\\.\\s*[^]*?(?=\\n\\d+\\.\\s|$)/g);\n\nif (!matches) {\n  throw new Error('Nenhuma objeção encontrada no formato esperado');\n}\n\nconst resultado = [];\n\nmatches.forEach((bloco, index) => {\n  const linhas = bloco.trim().split('\\n').filter(linha => linha.trim() !== '');\n  \n  if (linhas.length === 0) return;\n  \n  // Extrai informações da primeira linha\n  const primeiraLinha = linhas[0];\n  \n  // Tenta extrair número e pergunta com diferentes padrões\n  let match = primeiraLinha.match(/^(\\d+)\\.\\s*[\"\"]([^\"\"]+)[\"\"](.*)$/);\n  \n  if (!match) {\n    // Tenta sem aspas\n    match = primeiraLinha.match(/^(\\d+)\\.\\s*(.+)$/);\n  }\n  \n  if (!match) return;\n  \n  const numero = match[1];\n  let pergunta = match[2].trim();\n  \n  // Se há texto adicional após as aspas na primeira linha, adiciona à pergunta\n  if (match[3] && match[3].trim()) {\n    pergunta += ' ' + match[3].trim();\n  }\n  \n  // Procura por \"Resposta:\" nas linhas seguintes\n  const respostaIndex = linhas.findIndex(linha => linha.includes('Resposta:'));\n  \n  let resposta = '';\n  if (respostaIndex >= 0) {\n    // Junta todas as linhas da resposta\n    const respostaLinhas = linhas.slice(respostaIndex);\n    resposta = respostaLinhas.join(' ').replace(/^.*?Resposta:\\s*/, '').trim();\n  } else {\n    // Se não encontrar \"Resposta:\", pega tudo após a primeira linha\n    resposta = linhas.slice(1).join(' ').trim();\n  }\n  \n  // Cria o conteúdo completo\n  const conteudo = `${pergunta}\\nResposta: ${resposta}`;\n  \n  // Só adiciona se tiver conteúdo válido\n  if (pergunta.length > 0 && resposta.length > 0) {\n    resultado.push({\n      json: {\n        id: parseInt(numero),\n        numero: numero,\n        titulo: null,\n        pergunta: pergunta,\n        resposta: resposta,\n        conteudo: conteudo,\n        tamanho_texto: conteudo.length,\n        documentId: item.documentId || null,\n        texto_original: bloco.trim()\n      }\n    });\n  }\n});\n\n// Retorna array de objetos JSON - cada objeção será um item separado no n8n\nreturn resultado;"
      },
